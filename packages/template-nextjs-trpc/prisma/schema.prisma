generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Example {
    id        Int      @id @default(autoincrement())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
    archived   Boolean   @default(false)
    archivedAt DateTime?

    name      String

    @@index([name])
}

model User {
    id         String    @id @default(uuid())
    createdAt  DateTime  @default(now())
    updatedAt  DateTime  @updatedAt
    archived   Boolean   @default(false)
    archivedAt DateTime?

    email     String
    password  String
    firstName String
    lastName  String
}
