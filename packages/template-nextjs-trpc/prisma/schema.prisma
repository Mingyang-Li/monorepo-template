generator client {
  provider = "prisma-client-js"

  // Fixes issue of PrismaClient not found in monorepo setup
  // Issue: https://github.com/prisma/prisma/issues/6603
  output = "../../../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Example {
  id        Int      @id @default(autoincrement())
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@index([name])
}
